{
	"name": "mediumsans/kirby-lendmanagement",
	"description": "A lend management plugin for Kirby CMS",
    "license": "proprietary",
    "type": "kirby-plugin",
	"version": "1.0.0",
	"authors": [
		{
			"name": "Samuel Cardoso",
			"email": "samuel@mediumsans.studio"
		}
	],
	"require": {
		"getkirby/composer-installer": "^1.2",
        "php": ">=8.0.0",
        "chillerlan/php-qrcode": "dev-main",
        "beebmx/kirby-db": ">=2.0.0"
    },
    "autoload": {
        "psr-4": {
            "Beebmx\\": "site/",
            "MediumSans\\LendManagement\\": "classes/"
        }
    },
	"config": {
		"allow-plugins": {
			"getkirby/composer-installer": true
		}
	}
}
